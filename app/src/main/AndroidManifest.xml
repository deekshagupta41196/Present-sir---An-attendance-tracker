<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.deeksha.presentsir">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/download"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FacultyLogin"
            android:label="@string/title_activity_faculty_login" />
        <activity
            android:name=".StudentLogin"
            android:label="@string/title_activity_student_login"></activity>
        <activity
            android:name=".FacultyMenu"
            android:label="Menu"></activity>
        <activity
            android:name=".Add_Student"
            android:label="Add Student"></activity>
        <activity
            android:name=".Mark_Attendence"
            android:label="Mark Attendence"></activity>
        <activity android:name=".Delete_Student"
            android:label="Delete Student"></activity>
        <activity android:name=".AllTime">
        </activity>
        <activity android:name=".CrossCheck"></activity>
        <activity android:name=".Info"></activity>
    </application>

</manifest>